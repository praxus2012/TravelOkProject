
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <section class="secSuperior">
        <h1>TravelOk Admin</h1>
    </section>
    <section class="container secCentral">
        <div class="dvLogeo ">
            <ul class="ulCampLogeo">
                <li>
                    <h3 id="hBienv">Bienvenido</h3>
                </li>
                <li>
                    <input type="text" id="inUsuario" accept="a-z,A-Z,0-9" class="form-control" placeholder="Usuario" />
                </li>
                <li>
                    <input type="password" id="inContrasena" class="form-control" placeholder="Contraseña" />
                </li>
                <li>
                    <button class="btn btn-primary form-control" id="btnIniciarSesion">Inciar sesion</button>
                </li>
                <li>
                    <img id="imagenCargando" src="~/Img/loading.gif" />
                </li>
            </ul>
        </div>
    </section>
    <section class="secPie"></section>
    <input type="hidden" id="urlHome" value="@Url.Action("Index","Home")" />
    <input type="hidden" id="urlLogeo" value="@Url.Action("Logeo","Login")" />


    <script src="~/Scripts/Project/jQuery/jquery-3.4.1.js"></script>
    <script src="~/Scripts/Project/Bootstrap/bootstrap.js"></script>
    <script src="~/Scripts/Project/Bootstrap/bootstrap.bundle.js"></script>
    <script src="~/Scripts/Project/Bootstrap/bootstrap-select.js"></script>
    <script src="~/Scripts/Project/jsLogin.js"></script>

    <link href="~/Content/Project/Bootstrap/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/Project/Bootstrap/bootstrap-reboot.css" rel="stylesheet" />
    <link href="~/Content/Project/Bootstrap/bootstrap-grid.min.css" rel="stylesheet" />
    <link href="~/Content/Project/cssLogin.css" rel="stylesheet" />
</body>
</html>
